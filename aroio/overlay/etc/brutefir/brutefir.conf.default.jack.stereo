## INPUTS / OUTPUTS ##
input "left", "right" {
        device: "jack" { ports: "" , "" ; priority: 50 ; };
	sample: "AUTO";
        channels: 2;
};

output "left", "right" {
        device: "jack" { ports: "jackmixer:in1_left" , "jackmixer:in1_right" ; priority: 50 ; };
	sample: "AUTO";
        channels: 2;
        delay: 0 , 0;
	maxdelay: 8000;
};

## FILTERS ##
## L
filter "left" {
        from_inputs: "left";
        to_outputs:"left"/0/1;
	process: -1;
	coeff:"left0";
};

## R
filter "right" {
        from_inputs: "right";
        to_outputs:"right"/0/1;
	process: -1;
	coeff:"right0";
};
