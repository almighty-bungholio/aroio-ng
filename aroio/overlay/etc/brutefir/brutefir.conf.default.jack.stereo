## GENERAL ##
sampling_rate: 96000;
filter_length: 65535;
overflow_warnings: true;
show_progress: false;
max_dither_table_size: 0;
allow_poll_mode: false;
powersave: false;
monitor_rate: false;
lock_memory: true;
sdf_length: -1;

logic:
    ## Command line interface
    "cli" { port: 3000; };

