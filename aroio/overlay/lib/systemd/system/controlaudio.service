[Unit]
Description=Audio subsystem
After=syslog.target sound.target network.target bluetooth.target checkuserconfig.service checksoundcard.service
#Before=squeezelite.service gmediarender.service shairport-sync.service bluealsa-aplay.service brutefir.service
#Requires=audioplayers.target

[Service]
#TimeoutStartSec=3
Type=oneshot
#EnvironmentFile=/boot/userconfig.txt
ExecStart=/usr/bin/controlaudio start
#ExecStop=/usr/bin/controlaudio stop
#Restart=always

[Install]
WantedBy=multi-user.target
